{
    "name": "dipeshsukhia/laravel-generate-helpers",
    "description": "This package helps to generate and autoload custome heplers, It can generate multilevel helpers in context of directory.",
    "keywords": [
        "dipeshsukhia",
        "laravel-generate-helpers"
    ],
    "homepage": "https://github.com/dipeshsukhia/laravel-generate-helpers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dipesh Sukhia",
            "email": "dipesh.sukhia@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DipeshSukhia\\LaravelGenerateHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DipeshSukhia\\LaravelGenerateHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DipeshSukhia\\LaravelGenerateHelpers\\LaravelGenerateHelpersServiceProvider"
            ],
            "aliases": {
                "LaravelGenerateHelpers": "DipeshSukhia\\LaravelGenerateHelpers\\LaravelGenerateHelpersFacade"
            }
        }
    }
}

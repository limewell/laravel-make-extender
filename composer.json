{
    "name": "limewell/laravel-make-extender",
    "description": "This package helps to generate and autoload custom helpers, It can generate multilevel helpers in context of directory.",
    "keywords": [
        "limewell",
        "laravel-make-extender"
    ],
    "homepage": "https://github.com/limewell/laravel-make-extender",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dipesh Sukhia",
            "email": "dipesh.sukhia@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Bhavin Gajjar",
            "email": "gajjarbhavin22@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Limewell\\LaravelMakeExtender\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Limewell\\LaravelMakeExtender\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Limewell\\LaravelMakeExtender\\LaravelMakeExtenderServiceProvider"
            ],
            "aliases": {
                "LaravelMakeExtender": "Limewell\\LaravelMakeExtender\\LaravelMakeExtenderFacade"
            }
        }
    }
}
